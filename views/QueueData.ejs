<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    <title>Hello, world!</title>
  </head>
  <body>
    <%- include('partials/navbar') %>
   

      <div class="container mt-5 p-4 shadow-lg w-50">
        <h2 class="text-center">Select a Client</h2>
       

       
        <form action="/submit-form" method="post">
            <div class="form-group">
                <label for="clientSelect">Select a client:</label>
                <select id="clientSelect" name="clientSelect">
                  <% allUsers.forEach(user => { %>
                    <option value="<%= user.name %>"><%= user.name %></option>
                  <% }); %>
                </select>
              </div>
              
              <% allUsers.forEach(user => { %>
                <!-- Add a hidden input field for Data value -->
                <input type="hidden" name="<%= user.name %>_data" value="<%= user.data %>">
              <% }); %>
              
          
            <div class="form-group">
              <input type="text" placeholder="Sent" name="sent">
            </div>
          
            <div class="form-group">
              <input type="text" placeholder="Queue" name="queue">
            </div>
          
            <div class="form-group">
              <select name="selectbox" id="">
                <option value="default">select</option>
                <option value="A_airtel">A_airtel</option>
                <option value="M_airtel">M_airtel</option>
                <option value="BSNL">BSNL</option>
              </select>
            </div>
          
            <button>submit</button>
            <!-- Other form elements... -->
            <% if (typeof error !== 'undefined' && error) { %>
              <div class="error-message" style="color:red">
                  <%= error %>
              </div>
          <% } %>
          </form>
          
          
    </div>
    
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  </body>
</html>