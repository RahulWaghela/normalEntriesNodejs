  <body>
    <%- include('partials/navbar') %>

    <div class="parentDivOfALl">
      <!-- <div class="cont"> -->

        <!-- background-color: white;" -->
    <div class="container mt-5 p-4 shadow-lg" style="width: 50%;">
        <h2 class="text-center">Last update</h2>
       


        <form action="/sentReport" method="get">
          <label for="selectedDate">Select Date:</label>
          <input type="date" id="selectedDate" name="date" value="<%= selectedDate %>">
          <input id="semo" type="submit" value="Filter" onclick="showDate()">
        </form>
       <script>
       
       </script>
        <table class="table table-hover">
          <thead>
              <tr>
                  <th scope="col">#</th>
                  <th scope="col">Client</th>
                  <th scope="col">A_airtel</th>
                  <th scope="col">M_airtel</th>
                  <th scope="col">BSNL</th>
                  
              </tr>
          </thead>
          <tbody>
            <% latestEntries.forEach((user, index) => { %>
              <tr>
                <td><%= index + 1 %></td>
                <td><%= user._id %></td>
                <td><%= user.A_airtel || '' %></td>
                <td><%= user.M_airtel || '' %></td>
                <td><%= user.BSNL || '' %></td>
              </tr>
            <% }); %>
          </tbody>
      </table>
      
      

 
</div>
    




<!-- this is total of operator table -->
<div class="container mt-5 p-4 shadow-lg" style="width: 40%">
  <% // Calculate the total for A_airtel, M_airtel, and bsnl operators %>
  <% const totalAirtelA = latestEntries.filter(entry => entry.selectbox === 'A_airtel').reduce((total, entry) => total + entry.sent, 0); %>
  <% const totalAirtelM = latestEntries.filter(entry => entry.selectbox === 'M_airtel').reduce((total, entry) => total + entry.sent, 0); %>
  <% const totalBsnl = latestEntries.filter(entry => entry.selectbox === 'bsnl').reduce((total, entry) => total + entry.sent, 0); %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Operator</th>
      <th>Total Sent</th>
      <th>SIM Data</th>
      <th>Total SIM data</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>A_airtel</td>
      <td><%= totalAirtel_A %></td>
      <td><%= subtractedVAl1 %></td>
      <td><%= lastAirtel_A_100 %></td>
     
    </tr>
    <tr>
      <td>M_airtel</td>
      <td><%= totalAirtel_M %></td>
      <td><%= subtractedVAl2 %></td>
      <td><%= lastAirtel_M_100 %></td>
    </tr>
    <tr>
      <td>Bsnl</td>
      <td><%= totalBSNL %></td>
      <td><%= subtractedVAl3 %></td>
      <td><%= lastBSNL_100 %></td>
    </tr>
    <tr>
      <td>Total of All</td>
      <td><%= TotalOfALl %></td>
      <td><%= total %></td>
      <td><%= lastTotal %></td>
    </tr>

    <!-- <tr>
      <td>Sub</td>
      <td><%= subtractedVAl1 %></td>
    </tr>
    <tr>
      <td>Sub</td>
      <td><%= subtractedVAl2 %></td>
    </tr>
    <tr>
      <td>Sub</td>
      <td><%= subtractedVAl3 %></td>
    </tr> -->
  </tbody>
</table>
</div>
  
</div>
